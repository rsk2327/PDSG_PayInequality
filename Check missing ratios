# calculate missing ratio in one time
dataf = [df1,df2,df3,df4,df5,df6,df7,df8,df9,df10,df11,df12,df13,df14,df15,df16,df17,df18,f19,df20,df21,df22,df23,df24,df25,df26,df27,df28,df29]
for i in dataf:
    for j in i.columns:
        print(u'%s =' %j,(i[j].isna().sum())*100/len(i))

# calculate missing ratio of each file respectively
